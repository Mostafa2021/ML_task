   "source": [
    "import gradio as gr\n",
    "from joblib import load\n",
    "import pandas as pd\n",
    "\n",
    "def predict(text):\n",
    "    return \"Predicted Dialect :\" + str(load(\"text_classification.joblib\").predict(pd.Series([text]))[0])\n",
    "iface = gr.Interface(fn=predict, inputs=\"text\", outputs=\"text\")\n",
    "iface.launch()"
   ]
